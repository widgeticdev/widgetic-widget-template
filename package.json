{
  "name": "widget-base-template",
  "title": "Widget Base Template",
  "description": "Widget Base Template",
  "version": "0.0.0",
  "main": "lib/widget.js",
  "private": true,
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "ie": "11",
            "browsers": [
              "last 2 versions"
            ]
          },
          "modules": false,
          "useBuiltIns": true
        }
      ]
    ]
  },
  "scripts": {
    "start": "node server/server.js",
    "build-dev": "webpack --config webpack.config.dev.js",
    "build": "webpack --config webpack.config.prod.js",
    "stats": "webpack --config webpack.config.dev.js --profile --json > stats.json",
    "validate": "widgetic-tools validate lib",
    "version": "npm run validate && npm run build && git add -A lib",
    "postversion": "git push"
  },
  "dependencies": {
    "@widgetic/components": "^10.0.1",
    "@widgetic/editor": "^3.9.4",
    "@widgetic/sdk": "^1.6.4",
    "@widgetic/spine": "^2.1.1",
    "@widgetic/tools": "^1.0.0",
    "@widgetic/workbench": "^2.2.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.7",
    "debounce": "^1.2.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "geval": "^2.2.0",
    "prettier": "^2.0.5",
    "prettier-plugin-svelte": "^1.1.0",
    "style-loader": "^0.18.2",
    "svelte": "^3.16.5",
    "svelte-extras": "^2.0.2",
    "svelte-loader": "^2.13.6",
    "svelte-transitions": "^1.2.0",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-sources": "^1.4.3"
  }
}
